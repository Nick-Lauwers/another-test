$('.unlike-discussion').bind('ajax:success', function(){
   $(this).parent().parent().find('.vote-count').html('<%= escape_javascript @discussion.votes_for.size.to_s %>');
   $(this).closest('.unlike-discussion').hide();
   $(this).closest('.vote-icon').html(' <%= link_to like_discussion_path(@discussion), remote: true, method: :get, class: 'like-discussion' do %> <i class="fa fa-thumbs-o-up fa-lg like-icon" aria-hidden="true"></i> <% end %>');
});