<% @arrVehicles.each do |vehicle| %>
  <%= link_to vehicle do %>
    <li id="vehicle-<%= vehicle.id %>" class="list-item">
      <div class="row">
        <div class="search-image-container col-md-5">
          <div class="search-image", style="background-image:url(<%= vehicle.photos[0].image.url() if vehicle.photos.length > 0 %>)"></div>
        </div>
        <div class="list-item-text col-md-5">
          <h3 class="dealership"><%= vehicle.user.name %></h3>
          <h2><%= vehicle.listing_name %></h2>
          <h3 class="vehicle-description"><%= vehicle.summary %></h3>
        </div>
        <div class="price-details col-md-2">
          <h3>From</h3>
          <h2>$<%= vehicle.price %></h2>
          <div class="btn-list">
            View Vehicle
          </div>
        </div>
      </div>
    </li>
  <% end %>
<% end %>